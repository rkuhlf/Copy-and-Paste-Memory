{
  "manifest_version": 2,
  "name": "Copy and Paster Memory",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.1.min.js", "Page.js"]
  }
],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ]
}
